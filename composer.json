{
    "name": "dieze/dz-task-module",
    "description": "Module de gestion de tâches pour ZF2, compatible Doctrine",
    "type": "library",
    "keywords": [
        "zf2",
        "task",
        "tâche",
        "management",
        "gestion"
    ],
    "homepage": "https://github.com/dieze/DzTaskModule",
    "authors": [
        {
            "name": "Desfourneaux Adrien (aka Dieze)",
            "email": "dieze51@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-eventmanager": "~2.1",
        "zendframework/zend-filter": "~2.1",
        "zendframework/zend-form": "~2.1",
        "zendframework/zend-stdlib": "~2.1",
        "zendframework/zend-loader": "~2.1",
        "zendframework/zend-mvc": "~2.1",
        "zendframework/zend-modulemanager": "~2.1",
        "zendframework/zend-servicemanager": "~2.1",
        "zendframework/zend-view": "~2.1"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "phpunit/php-code-coverage": ">=1.2.10,<1.3.0",
        "phpspec/phpspec": "2.0.*@dev",
        "henrikbjorn/phpspec-code-coverage" : "dev-master",
        "phpdocumentor/phpdocumentor": "2.*",
        "squizlabs/php_codesniffer": "1.*",
        "fabpot/php-cs-fixer": "dev-master",
        "phpmd/phpmd": "1.5.*",
        "pdepend/pdepend": "1.1.1",
        "phploc/phploc": "*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-0": {
            "DzTaskModule": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
